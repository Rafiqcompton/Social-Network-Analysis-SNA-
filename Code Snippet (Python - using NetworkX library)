     import networkx as nx

     # Create a graph
     G = nx.Graph()

     # Add nodes
     G.add_node("Alice")
     G.add_node("Bob")
     G.add_node("Charlie")

     # Add edges
     G.add_edge("Alice", "Bob")
     G.add_edge("Bob", "Charlie")

     # Calculate centrality measures
     degree_centrality = nx.degree_centrality(G)
     betweenness_centrality = nx.betweenness_centrality(G)
